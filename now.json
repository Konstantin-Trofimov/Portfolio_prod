{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "nodemailer.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "app.js"
        },
        {
            "src": "app.js",
            "methods": [
                "POST",
                "GET"
            ],
            "dest": "app.js"
        }
    ]
}